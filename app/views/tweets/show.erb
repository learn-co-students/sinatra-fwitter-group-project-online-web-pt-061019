<h3>Your new Tweet:</h3>
<p><%=@tweet.id%>. <%=@tweet.content%></p>

<form action="/tweets/<%=@tweet.id%>/delete" method="POST">
  <input type="hidden" id="hidden" name="_method" value="DELETE">
  <input type="submit" name="submit" value="Delete Tweet">
</form>

<form action="/tweets/<%=@tweet.id%>" method="POST">
  <input type="hidden" name="_method" id="hidden" value="PATCH">
  <input type="text" name="content" placeholder="<%=@tweet.content%>" id ="content">
  <input type="submit" id="submit" value="Edit Tweet">
</form>
