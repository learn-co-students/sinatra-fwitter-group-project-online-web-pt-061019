<h3>Tweet Tweet</h3>
<h4>Welcome, <%= current_user.username %></h4>
  <% @tweets.each do |tweet| %>
    <p><%= tweet.content%></p>
  <% end %>

<button type="button" name="button"><a href="/tweets/new">New Tweet</a></button>
<button type="button" name="button"><a href="/logout">Log out</a></button>
